#INCLUDE "configs_llvm_ir/return_typed.fg" (i1 i8 i16 i32 i64)

@INSTRUCTION
#BEGIN_RULE
    @INTEGRAL_INSTRUCTION
#END_RULE
#BEGIN_RULE
    @ICMP_INSTRUCTION
#END_RULE
#BEGIN_RULE:4
    @POINTER_INSTRUCTION
#END_RULE

@INTEGRAL_INSTRUCTION
